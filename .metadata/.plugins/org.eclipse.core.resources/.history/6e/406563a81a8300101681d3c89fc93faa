package JDBC.TestDao;

import JDBC.dao.UserDao;
import JDBC.dao.UserDaoImpl;
import JDBC.model.Users;

public class TestDao {
	public static void main(String[] args) {
		UserDao userDao = new UserDaoImpl();

		// Thử lấy user theo username
		Users user = userDao.get("admin");

		if (user != null) {
			System.out.println("Login OK: " + user.getUserName() + " - " + user.getPassWord());
		} else {
			System.out.println("User not found!");
		}
	}
}
